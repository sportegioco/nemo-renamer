msgid "/num,5/ => 00001, 00002, 00003 , ..."
msgstr "{num5} => 00001, 00002, 00003, ..."

#: nautilus-renamer.py:136
#, fuzzy
msgid "/num,3+5/ => 005, 006, 007 , ..."
msgstr "{num5+5} => 00005, 00006, 00007, ..."

#: nautilus-renamer.py:137
msgid "A random number from 10 to 99"
msgstr "Número al azar del 10 al 99"

#: nautilus-renamer.py:138
msgid "The first three letters of filename."
msgstr "Las tres primeras letras del archivo."

#: nautilus-renamer.py:159
msgid "_Extension"
msgstr "_Extensión"

#: nautilus-renamer.py:160
msgid "_Recursive"
msgstr "_Recursivo"

#: nautilus-renamer.py:161
msgid "Also operate on extensions"
msgstr "También aplica sobre las extensiones de los archivos"

#: nautilus-renamer.py:162
msgid "Also operate on subfolders and files"
msgstr "También aplica a subdirectorios y archivos"

#: nautilus-renamer.py:170
msgid "Refresh Previe_w"
msgstr "_Actualiza previsualización"

#: nautilus-renamer.py:184
msgid "Original Name"
msgstr "Nombre Original"

#: nautilus-renamer.py:192
msgid "New Name"
msgstr "Nombre Nuevo"

#: nautilus-renamer.py:206
msgid "Pre_view"
msgstr "Pre_visualización"

#: nautilus-renamer.py:277
msgid "Enter the pattern here ... "
msgstr "Ingrese el patrón aquí ... "

#: nautilus-renamer.py:288
msgid "Enter the base pattern to rename. Click '?' for available patterns."
msgstr ""
"Introduzca el patron base a renombrar. Haga click en '?' para ver los "
"patrones disponibles."

#: nautilus-renamer.py:303
msgid "Delete or replace multiple characters and words..."
msgstr "Borre o reemplace múltiples caracteres y palabras..."

#: nautilus-renamer.py:307
msgid "Words to be replaced separated by \"/\", e.g., 1/2 ..."
msgstr "Palabras a ser reemplazadas separadas por \"/\", ej., 1/2 ..."

#: nautilus-renamer.py:311
msgid "Corresponding words to replace with, e.g., one/two ..."
msgstr "Palabras de reemplazo separadas por \"/\", ej., uno/dos ..."

#: nautilus-renamer.py:314
msgid "Enter the characters or words to be replaced, separated by '/'"
msgstr "Inserte los caracteres o palabras a reemplazar, separados mediante '/'"

#: nautilus-renamer.py:315
msgid "Enter the corresponding words to replace with"
msgstr "Ingrese las correspondientes palabras de reemplazo"

#: nautilus-renamer.py:327
msgid "Keep Original Case"
msgstr "Conservar mayúsculas y minúsculas originales"

#: nautilus-renamer.py:328
msgid "ALL IN CAPITALS"
msgstr "TODO EN MAYÚSCULAS"

#: nautilus-renamer.py:329
msgid "all in lower case"
msgstr "todo en minúsculas"

#: nautilus-renamer.py:330
msgid "First letter upper case"
msgstr "Primera letra en mayúsculas"

#: nautilus-renamer.py:331
msgid "Title Case"
msgstr "Todas Las Iniciales En Mayúsculas"

#: nautilus-renamer.py:332
msgid "Capital Case After ..."
msgstr "Mayúsculas a continuación de ..."

#: nautilus-renamer.py:353
msgid "Choose One"
msgstr "Elija una"

#: nautilus-renamer.py:363
msgid "Choose the casing style you want to apply."
msgstr "Elija el estilo de mayúsculas/minúsculas que desee aplicar."

